[application]

config/name="Crop Carnage"
run/main_scene="res://menu/menu.tscn"
boot_splash/show_image=false
config/icon="res://icon.png"

[autoload]

EventBus="*res://autoloads/event_bus.gd"
Score="*res://autoloads/score.gd"
GlobalAudio="*res://autoloads/global_audio.gd"

[display]

window/size/width=200
window/size/height=200
window/size/resizable=false
window/size/test_width=800
window/size/test_height=800
window/stretch/mode="2d"
window/stretch/aspect="keep"

[global]

polyphony=5

[gui]

common/drop_mouse_on_gui_input_disabled=true
theme/custom_font="res://fonts/VT323.tres"

[importer_defaults]

texture={
